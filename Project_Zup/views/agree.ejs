<!-- 상단 텝 스타일 -->
<style>
body {
	margin-top: 40px;
}

.stepwizard-step p {
	margin-top: 10px;
}

.stepwizard-row {
	display: table-row;
}

.stepwizard {
	display: table;
	width: 50%;
	position: relative;
}

.stepwizard-step button[disabled] {
	opacity: 1 !important;
	filter: alpha(opacity = 100) !important;
}

.stepwizard-row:before {
	top: 14px;
	bottom: 0;
	position: absolute;
	content: " ";
	width: 100%;
	height: 1px;
	background-color: #ccc;
	z-order: 0;
}

.stepwizard-step {
	display: table-cell;
	text-align: center;
	position: relative;
}

.btn-circle {
	width: 30px;
	height: 30px;
	text-align: center;
	padding: 6px 0;
	font-size: 12px;
	line-height: 1.428571429;
	border-radius: 15px;
}
</style>

</head>

<body>
	
<header>
 <nav id="mainNav" class="navbar navbar-default navbar-custom navbar-fixed-top" 
 style="background-color: #054229; height: 90px; z-index: 3;"> 
 <a class="navbar-brand page-scroll" href="/"
					style="font-size: 70px; margin-top: 0.02em; color: rgb(18, 155, 5);"> Z U P 
					<span style="font-size: 30px; color:white;"> 회원가입</span></a>
					
 </nav>



</header>

	<!-- 회원가입 상단 메뉴 -->
	<div class="container" style="margin-top: 10em;">

		<div class="stepwizard col-md-offset-3">
			<div class="stepwizard-row setup-panel">
				<div class="stepwizard-step">
					<a href="#step-1" type="button" class="btn btn-primary btn-circle" 
					style="background-color: rgb(18, 155, 5)">1</a>
					<p>이용 약관</p>
				</div>
				<div class="stepwizard-step">
					<a href="#step-2" type="button" class="btn btn-default btn-circle"
						disabled="disabled">2</a>
					<p>회원 가입</p>
				</div>
				<div class="stepwizard-step">
					<a href="#step-3" type="button" class="btn btn-default btn-circle"
						disabled="disabled">3</a>
					<p>가입 완료</p>
				</div>

			</div>
		</div>

		<!-- 이용약관 폼 -->
		<div class="row setup-content" id="step-1" style="margin-top: 5em;">
			<div class="col-xs-6 col-md-offset-3">


				<form class="form-horizontal" method="post" onsubmit="return formSubmit2(this);">
					<div class="form-group">
						<div class="col-lg-12" id="provision">
							<label for="provision" class="col-lg-3 control-label">
							<span style="font-size: 20px;">회원가입약관</span></label>
							<!-- 회원가입 약관 -->
							<textarea class="form-control" rows="8" style="resize: none">
                        <% include ../include/agree.ejs %>                    
                    </textarea>

							<div class="col-xs-12">
								<div class="checkbox pull-right">
									<label for="join_user_agree"> <input type="checkbox"
										name="join_user_agree" id="join_user_agree" value="1" /> 회원가입
										약관에 동의합니다.
									</label>
								</div>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-lg-12" id="memberInfo">
							<label for="memberInfo" class="col-lg-3 control-label">
							<span style="font-size: 20px;">개인정보취급</span></label>
							<!-- 개인정보 취급 방침 -->
							<textarea class="form-control" rows="8" style="resize: none">
                           <% include ../include/agree2.ejs %> 
                    </textarea>
							<div class="col-xs-12">
								<div class="checkbox pull-right">
									<label for="join_priv_agree"> <input type="checkbox"
										name="join_priv_agree" id="join_priv_agree" value="1" /> 개인정보
										처리방침에 동의합니다.
									</label>
								</div>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12 text-center">
							<button class="btn btn-success  btn-lg " type="submit" id="agreeButton">Next</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>

	<!-- 약관 동의 유효성 검사 -->

<script type="text/javascript">
function formSubmit2(form){
	

		
		if($('input:checkbox[name="join_user_agree"]').is(":checked")){
		    console.log("체크됨");
			}else{
				swal({
					  title : '회원가입 오류!',
					  text : '회원가입 약관에  동의해주세요!',
					  type :'warning',
					  confirmButtontext : '닫기'
			})
			$("#join_user_agree").focus();
			return false;
			}
		
		if($('input:checkbox[name="join_priv_agree"]').is(":checked")){
			console.log("체크됨");
		    }else{
			      swal({
					  title : '개인정보 오류!',
					  text : '개인정보 방침에 동의해주세요!',
					  type :'warning',
					  confirmButtontext : '닫기'
			})
			$("#join_priv_agree").focus();
			return false;
		}
		
	}
</script>


	<script src="https://code.jquery.com/jquery-1.9.1.js"
		type="text/javascript"></script>
	<script
		src="https://netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>

</body>

