  <script type="text/javascript">
			window.onload = function() {
				var button = document.getElementById('conButton');

				//컨텍트어스	
				button.onclick = function() {
					var id = "<%= sessionId%>";

					if (id == "") {
						swal({
							title : '잠시만!',
							text : '먼저 로그인을 해주세요!',
							type : 'warning'
						})

					} else {
						location.href = "contact"
					}

				}
				

			};
		</script>

